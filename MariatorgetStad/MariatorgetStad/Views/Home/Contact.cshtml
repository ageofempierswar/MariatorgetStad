@model MariatorgetStadDAL.Models.Contact

@using (Html.BeginForm("Contact", "Contact", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

            <div id="googleMap">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2225.852666219539!2d13.735911199999999!3d56.0904661!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4653fc42d08cd8c5%3A0xfffcaaed3a132cbc!2sN%C3%B6sdala+6030%2C+281+92+H%C3%A4ssleholm!5e0!3m2!1ssv!2sse!4v1432725690555" width="600" height="450" frameborder="0" style="border:0"></iframe>
            </div>

            <div id="contact">
                <div class="form-group">

                    @Html.LabelFor(model => model.EmailAdress, htmlAttributes: new { })
                    <div>
                        @Html.EditorFor(model => model.EmailAdress, new { htmlAttributes = new { @class = "form-control", @rows = 8 } })
                        @Html.ValidationMessageFor(model => model.EmailAdress, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">

                    @Html.LabelFor(model => model.Subject, htmlAttributes: new { })
                    <div>
                        @Html.EditorFor(model => model.Subject, new { htmlAttributes = new { @class = "form-control", @rows = 8 } })
                        @Html.ValidationMessageFor(model => model.Subject, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">

                    @Html.LabelFor(model => model.Message, htmlAttributes: new { })
                    <div>
                        @Html.EditorFor(model => model.Message, new { htmlAttributes = new { @class = "form-control", @rows = 8 } })
                        @Html.ValidationMessageFor(model => model.Message, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    <div>
                        <input type="submit" class="btn btn-default" value="Send" />
                    </div>
                </div>
            </div>

}